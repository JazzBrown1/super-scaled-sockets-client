{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["./src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "./jsdoc/docdash",
        "encoding": "utf8",
        "destination": "jsdoc/out",
        "recurse": true,
        "verbose": true,
        "readme": "./README.md"
    },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "docdash": {
    "menu":{
      "Getting Started":{
            "href":"index.html#getting-started",
            "class":"menu-item",
            "id":"website_link"
        },
        "Server Docs":{
              "href":"https://super-scaled-sockets.tezle.com/docs/server",
              "target":"_blank",
              "class":"menu-item",
              "id":"website_link"
          },
      "Github":{
          "href":"https://github.com/JazzBrown1/super-scaled-sockets",
          "target":"_blank",
          "class":"menu-item",
          "id":"forum_link"
      }
    },
    "static":true,
    "search": true,
    "sectionOrder": [
      "Modules",
      "Classes",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Tutorials",
      "Interfaces"
      ],
      "meta": {
        "title": "Super Scaled Sockets",
        "description": "Super scaling websocket servers",
        "keyword": "super scaled sockets websockets"
      }
  }
}