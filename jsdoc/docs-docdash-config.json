{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["./src", "../super-scaled-sockets/src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "jsdoc/out",
        "recurse": true,
        "verbose": true,
        "readme": "./jsdoc/README.md"
    },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "docdash": {
    "menu":{
      "Project Website":{
            "href":"https://myproject.com",
            "target":"_blank",
            "class":"menu-item",
            "id":"website_link"
        },
      "Forum":{
          "href":"https://myproject.com.forum",
          "target":"_blank",
          "class":"menu-item",
          "id":"forum_link"
      }
    },
    "static":true,
    "search": true,
    "sectionOrder": [
      "Modules",
      "Classes",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Tutorials",
      "Interfaces"
      ],
      "meta": {
        "title": "Super Scaled Sockets",
        "description": "Super scaling websocket servers",
        "keyword": "super scaled sockets websockets"
      }
  }
}