{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "./src",
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "jsdoc/out",
        "recurse": true,
        "verbose": true,
        "readme": "./jsdoc/README.md"
    },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "docdash": {
    "static":true,
    "search": true,
    "sectionOrder": [
      "Modules",
      "Classes",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Tutorials",
      "Interfaces"
      ],
      "meta": {
        "title": "Super Scaled Sockets",
        "description": "Super scaling websocket servers",
        "keyword": "super scaled sockets websockets"
      }
  }
}